proc:1 a tb:Process;
  rdfs:label "Boil mash, strain, boil, restrain, hops in wort";
  dc:description """Boil the mash. Then run off in strainer. Boil the
  wort, then strain through strainer one or more times. Hops boiled in
  wort. Then cool and pitch.""";
  tb:cold-mash false;
  tb:infusion-mash-steps 0;
  tb:mash-circulation-rounds 0;
  tb:mash-kettle-heated true;
  tb:mash-boiled true;

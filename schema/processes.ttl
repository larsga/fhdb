# ===== HOP TREATMENTS

tb:boil-hops-in-wort a tb:HopTreatment.
tb:humlebeit a tb:HopTreatment;
  dc:description "Boil hops in a small part of the wort.".
tb:hop-tea a tb:HopTreatment;
  dc:description "Boil hops in water, then add.".

# FIXME: do we really need both of these?
tb:hops-in-mash a tb:HopTreatment;
  dc:description "Put the hops in the mash, rely on mashing for extraction.".
tb:boil-hops-in-mash a tb:HopTreatment;
  dc:description "Boil the mash with the hops in it.".
# FIXME: could have hop-in-oven-mash as a treatment?

tb:dry-hopping a tb:HopTreatment;
  dc:description "Put the hops in the fermentor.".
tb:lauter-through-hops a tb:HopTreatment;
  dc:description "Lautered wort runs through/onto the hops.".

# ===== MASH HEATING METHODS

tb:mixed a tb:MashHeatingMethod;
  rdfs:label "Mixed heating";
  dc:description "Several different methods combined with no primary.".
tb:oven a tb:MashHeatingMethod;
  rdfs:label "Oven heating";
  dc:description "Cold/lukewarm water and malt heated in the oven.".
tb:infusion a tb:MashHeatingMethod;
  rdfs:label "Infusion heating";
  dc:description "Liquid heated externally and poured on.".
tb:kettle a tb:MashHeatingMethod;
  rdfs:label "Kettle heating";
  dc:description "Cold/lukewarm water + malt heated in kettle.".
tb:unknown a tb:MashHeatingMethod;
  rdfs:label "Unknown heating";
  dc:description "We simply don't know.".
tb:external a tb:MashHeatingMethod;
  rdfs:label "External heating";
  dc:description "The liquid is run off, heated, and poured back.".

